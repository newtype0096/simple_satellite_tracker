<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Main_Line_Color">#3f3f46</Color>

    <SolidColorBrush x:Key="Main_Background" Color="#252526" />
    <SolidColorBrush x:Key="Main_Line" Color="{StaticResource Main_Line_Color}" />

    <SolidColorBrush x:Key="Sub_Background" Color="#2d2d30" />

    <SolidColorBrush x:Key="Third_Background" Color="#3e3e42" />

    <SolidColorBrush x:Key="OrbitGrid_Line"
                     Opacity="0.3"
                     Color="{StaticResource Main_Line_Color}" />
</ResourceDictionary>